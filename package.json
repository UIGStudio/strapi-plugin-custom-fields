{
    "name": "@uigstudio/strapi-plugin-custom-fields",
    "private": false,
    "version": "0.1.0",
    "description": "This plugin add custom fields",
    "license": "MIT",
    "scripts": {
        "format": "prettier --write \"./**/*.+(js|jsx)\"",
        "lint": "eslint \"./**/*.+(js|jsx)\"",
        "lint:fix": "eslint --fix \"./**/*.+(js|jsx)\""
    },
    "strapi": {
